<!DOCTYPE service-builder SYSTEM "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.liferay.content.targeting.report.campaign.newsletter">
	<namespace>CT_ABReport</namespace>

	<entity name="ABVersion" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="abversionId" type="long" primary="true"/>

		<!-- Other fields -->

		<column name="campaignId" type="long"/>
		<column name="alias" type="String"/>
		<column name="viewsCount" type="int"/>
		<column name="goalCount" type="int"/>


		<column name="modifiedDate" type="Date"/>

		<!-- Order -->

		<order by="desc">
			<order-column name="modifiedDate"/>
		</order>

		<!-- Finder methods -->

		<finder name="CampaignId" return-type="Collection">
			<finder-column name="campaignId"/>
		</finder>
	</entity>
</service-builder>